<?xml version="1.0" encoding="UTF-8"?>
<Schema xmlns="http://www.w3.org/shex/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" exclude-prefixes="xsi shex" xsi:schemaLocation="http://www.w3.org/shex/ ../xsd/ShEx.xsd">
    <shape label="http://a.example/EntityShape">
        <group>
            <tripleConstraint predicate="http://a.example/p1"/>
            <tripleConstraint predicate="http://a.example/p2" min="0" objectShape="http://a.example/EmployeeShape"/>
        </group>
    </shape>
    <shape label="http://a.example/PersonShape">
        <tripleConstraint predicate="http://a.example/p3" objectShape="http://a.example/EntityShape"/>
    </shape>
    <shape label="http://a.example/OtherShape">
        <tripleConstraint predicate="http://a.example/p4"/>
    </shape>
    <shape label="http://a.example/EmployeeShape">
        <tripleConstraint predicate="http://a.example/p5" objectShape="http://a.example/PersonShape"/>
    </shape>
</Schema>